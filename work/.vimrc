unlet! skip_defaults_vim
source $VIMRUNTIME/defaults.vim

set nu
set relativenumber 
set ic
set mouse=c
set hlsearch
set autoread
set noswapfile
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
set autoindent

nnoremap J <C-o>
nnoremap K <C-i>

nnoremap H gT
nnoremap L gt

nnoremap <C-j> 10j
nnoremap <C-k> 10k
